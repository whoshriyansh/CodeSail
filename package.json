{
  "name": "codesail",
  "displayName": "codesail",
  "description": "Extension made by reverse enginginerring  the traycer  AI Extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:codesailView"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codesailSidebar",
          "title": "CodeSail",
          "icon": "./src/icons/CodeSail.svg"
        }
      ]
    },
    "views": {
      "codesailSidebar": [
        {
          "id": "codesailView",
          "name": "CodeSail View",
          "type": "webview"
        }
      ]
    },
    "commands": {
      "command": "show.files",
      "title": "Show Files"
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build-webview": "cd webview-ui && npm run build"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "groq-sdk": "^0.33.0",
    "openai": "^6.2.0"
  }
}
